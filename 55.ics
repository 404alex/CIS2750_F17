CREATE TABLE ORGANIZER(org_id INTEGER NOT NULL AUTO_INCREMENT PRIMARY KEY,name VARCHAR(60) NOT NULL,contact VARCHAR(60) NOT NULL);CREATE TABLE EVENT(event_id INTEGER NOT NULL AUTO_INCREMENT PRIMARY KEY,summary VARCHAR(60) NOT NULL,start_time DATETIME NOT NULL,location VARCHAR(60),organizer INTEGER REFERENCES ORGANIZER(org_id) ON DELETE CASCADE,num_alarms INTEGER NOT NULL DEFAULT 0);